<div class="auth-wrapper">
  <div class="auth-card">
    <div class="my-5 flex justify-center">
      <img src="/img/logo/logo.png" alt="logo" width="200" />
    </div>
    <% _.forOwn(messages, function(msg, color) { %>
    <div class="row">
      <div class="col s12">
        <div class="card-panel <%= color %>">
          <span class="white-text"><%= msg %></span>
        </div>
      </div>
    </div>
    <% }) %>

    <div class="row">
      <div class="row">
        <div class="col s12">
          <div class="card-panel">
            <p style="text-align: justify;">Selamat datang di layanan kami, silahkan masuk terlebih dahulu</p>
          </div>
        </div>
      </div>
      <div class="col s12 center">
        <a href="<%= authUrl + getBaseUrl() + "auth/login" %>" class="waves-effect waves-light btn green"><i class="material-icons right">login</i> Masuk</a>
      </div>
    </div>
  </div>
</div>